.container-place{
  margin: 60rpx 40rpx;
}

.body-item{
  display: flex;
  flex-direction: column;
}

.place-title{
  margin: 30rpx;
}

.title{
  font-size: 50rpx;
  font-weight: 500;
}

.desc{
  margin-left: 50rpx;
}

.place-wrap{
  display: flex;
  flex-wrap: wrap;
}

.bg-place{
  margin: 30rpx;
  background-color: #6bc099;
  border-radius: 15rpx;
  padding: 5rpx 10rpx;
  color: #ffffff;
}

